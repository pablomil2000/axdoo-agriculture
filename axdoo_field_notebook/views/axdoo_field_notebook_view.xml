                  name="Nursery"
                  action="action_nursery_contacts"
                  sequence="7"
                  groups="axdoo_field_notebook.group_axdoo_field_notebook"/>

        <menuitem id="menu_phytosanitary_application_type_form"
                  name="Phytosanitary Application Type"
                  action="field_notebook_phytosanitary_application_type_action"
                  sequence="8"
                  groups="axdoo_field_notebook.group_axdoo_field_notebook"/>

        <menuitem id="menu_agents_form"
                  name="Agents"
                  action="field_notebook_agent_action"
                  sequence="9"
                  groups="axdoo_field_notebook.group_axdoo_field_notebook"/>

    </menuitem>

    <menuitem id="field_notebook_location_menu"
              name="Location"
              parent="field_notebook_configuration_menu"
              sequence="2"
              groups="axdoo_field_notebook.group_axdoo_field_notebook">

        <menuitem id="field_notebook_autonomy_menu"
                  name="Autonomy"
                  action="field_notebook_autonomy_action"
                  sequence="1"
                  groups="axdoo_field_notebook.group_axdoo_field_notebook"/>

        <menuitem id="field_notebook_province_menu"
                  name="Provinces"
                  action="field_notebook_province_action"
                  sequence="2"
                  groups="axdoo_field_notebook.group_axdoo_field_notebook"/>

        <menuitem id="field_notebook_town_menu"
                  name="Towns"
                  action="field_notebook_town_action"
                  sequence="3"
                  groups="axdoo_field_notebook.group_axdoo_field_notebook"/>

    </menuitem>

    <menuitem id="field_notebook_machinery_menu"
              name="Machinery"
              parent="field_notebook_configuration_menu"
              sequence="3"
              groups="axdoo_field_notebook.group_axdoo_field_notebook">

        <menuitem id="field_notebook_equipment_implement_menu"
                  name="Implements"
                  action="field_notebook_equipment_implement_action"
                  sequence="1"
                  groups="axdoo_field_notebook.group_axdoo_field_notebook"/>

        <menuitem id="menu_product_form"
                  name="Products"
                  action="product.product_template_action"
                  sequence="2"
                  groups="axdoo_field_notebook.group_axdoo_field_notebook"/>

        <menuitem id="menu_equipment_form"
                  name="Equipments"
                  action="field_notebook_equipment_action"
                  groups="maintenance.group_equipment_manager,base.group_user"
                  sequence="3"/>

