<odoo>

    <record id="field_notebook_equipment_action" model="ir.actions.act_window">
        <field name="name">Equipments</field>
        <field name="res_model">maintenance.equipment</field>
        <field name="view_mode">tree,form,kanban</field>
        <field name="view_id" ref="maintenance.hr_equipment_view_tree"/>
    </record>

    <menuitem name="Field Notebook"
              id="axdoo_field_notebook_menu"
              sequence="10"
              web_icon="axdoo_field_notebook,static/description/icon.png"
              groups="axdoo_field_notebook.group_axdoo_field_notebook"/>

    <!-- Performance-->

    <menuitem id="field_notebook_performance_menu"
              name="Performances"
              parent="axdoo_field_notebook_menu"
              sequence="1"
              groups="axdoo_field_notebook.group_axdoo_field_notebook">

        <menuitem id="field_notebook_phytosanitary_menu"
                  name="Phytosanitary"
                  action="field_notebook_phytosanitary_action"
                  sequence="1"
                  groups="axdoo_field_notebook.group_axdoo_field_notebook"/>

        <menuitem id="field_notebook_labor_menu"
                  name="Labor"
                  action="field_notebook_labor_action"
                  sequence="2"
                  groups="axdoo_field_notebook.group_axdoo_field_notebook"/>

        <menuitem id="field_notebook_harvest_menu"
                  name="Harvest"
                  action="field_notebook_harvest_action"
                  sequence="3"
                  groups="axdoo_field_notebook.group_axdoo_field_notebook"/>

        <menuitem id="field_notebook_irrigation_menu"
                  name="Irrigation"
                  action="field_notebook_irrigation_action"
                  sequence="4"
                  groups="axdoo_field_notebook.group_axdoo_field_notebook"/>

    </menuitem>

    <!-- Structure-->

    <menuitem id="field_notebook_structure_menu"
              name="Structure"
              parent="axdoo_field_notebook_menu"
              sequence="2"
              groups="axdoo_field_notebook.group_axdoo_field_notebook">

        <menuitem id="field_notebook_associated_menu"
                  name="Associated"
                  action="action_associate_contacts"
                  sequence="1"
                  groups="axdoo_field_notebook.group_axdoo_field_notebook"/>

        <menuitem id="field_notebook_exploitation_menu"
                  name="Exploitation"
                  action="field_notebook_exploitation_action"
                  sequence="2"
                  groups="axdoo_field_notebook.group_axdoo_field_notebook"/>

        <menuitem id="field_notebook_ucth_menu"
                  name="U.C.T.H."
                  action="field_notebook_ucth_action"
                  sequence="3"
                  groups="axdoo_field_notebook.group_axdoo_field_notebook"/>

        <menuitem id="field_notebook_parcel_menu"
                  name="Parcel"
                  action="field_notebook_parcel_action"
                  sequence="4"
                  groups="axdoo_field_notebook.group_axdoo_field_notebook"/>

    </menuitem>

    <!-- Configuration-->

    <menuitem id="field_notebook_configuration_menu"
              name="Configuration"
              parent="axdoo_field_notebook_menu"
              sequence="3"
              groups="axdoo_field_notebook.group_axdoo_field_notebook"/>

    <menuitem id="axdoo_field_notebook_menu_configuration"
              name="Settings"
              parent="field_notebook_configuration_menu"
              sequence="0"
              action="axdoo_field_notebook_config_settings_action"
              groups="base.group_system"/>

    <menuitem id="field_notebook_parcel_ucth_menu"
              name="Parcel / U.C.T.H."
              parent="field_notebook_configuration_menu"
              sequence="1"
              groups="axdoo_field_notebook.group_axdoo_field_notebook">

        <menuitem id="field_notebook_campaign_menu"
                  name="Campaign"
                  action="field_notebook_campaign_action"
                  sequence="1"
                  groups="axdoo_field_notebook.group_axdoo_field_notebook"/>

        <menuitem id="field_notebook_crop_menu"
                  name="Crop"
                  action="field_notebook_crop_action"
                  sequence="2"
                  groups="axdoo_field_notebook.group_axdoo_field_notebook"/>

        <menuitem id="field_notebook_plot_use_menu"
