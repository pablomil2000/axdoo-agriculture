                  name="Agricultural system"
                  action="field_notebook_system_action"
                  sequence="5"
                  groups="axdoo_field_notebook.group_axdoo_field_notebook"/>

        <menuitem id="field_notebook_irrigation_system_menu"
                  name="Irrigation system"
                  action="field_notebook_irrigation_system_action"
                  sequence="6"
