    <record id="view_field_notebook_plot_use_tree" model="ir.ui.view">
        <field name="name">field.notebook.plot.use.tree</field>
        <field name="model">field.notebook.plot.use</field>
        <field name="priority">1</field>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <field name="sequence" widget="handle"/>
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="field_notebook_plot_use_action"
            model="ir.actions.act_window">
        <field name="name">Plot Use</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">field.notebook.plot.use</field>
        <field name="view_mode">tree,form,kanban</field>
        <field name="view_id"
               ref="view_field_notebook_plot_use_tree"/>
    </record>

</odoo>
